{
  "problemMatcher": [
    {
      "owner": "rust",
      "pattern": [
        {
          "regexp": "^(warning|warn|error)(\\[(.*)\\])?: (.*)$",
          "severity": 1,
          "message": 4,
          "code": 3
        },
        {
          "regexp": "^[\\s->=]*(.*):(\\d*):(\\d*)$",
          "file": 1,
          "line": 2,
          "column": 3
        }
      ]
    }
  ]
}
